<div class="container">
  <%= render partial: 'shared/product_with_btn_pricing' %>

  <%#= render partial: 'shared/reviews_section' #%>
  <%= render partial: 'shared/reviews_section_tab' %>

  <%= render partial: 'shared/security_and_trust' %>

  <div id="features" class="features row">
    <%#=image_tag 'compare/features.png' #%>

    <% @features.each do |item| %>
    <div class="col-xs-12 col-sm-6 col-md-4">
      <%= render partial: 'shared/feature', locals: {item: item} %>
    </div>
    <% end %>
  </div>

  <%#= render partial: 'shared/pricing' #%>
</div>

<div class="container-fluid bg2">
  <div class="container">
    <%= render partial: 'shared/app_downloads' %>
  </div>
</div>